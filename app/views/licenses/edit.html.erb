<% provide(:title, "Actualizar licencia") %>
<h1>Actualizar licencia</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@license) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name, "Nombre" %>
      <%= f.text_field :name %>

      <%= f.label :description, "Descripcion" %>
      <%= f.text_field :description %>

      <%= f.submit "Guardar cambios", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
